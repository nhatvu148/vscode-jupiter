{
  "name": "jupiter",
  "displayName": "JUPITER",
  "description": "Jupiter Language Support",
  "version": "1.1.5",
  "publisher": "nhatvu148",
  "license": "MIT",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.51.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nhatvu148/vscode-jupiter"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "jupiter"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.gotoDocument",
        "title": "Go to Document"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Go to Document configuration",
      "properties": {
        "goto-documentation.customDocs": {
          "type": "object",
          "default": {},
          "description": "Extend docs url for unsupported languages. ex. {'file.ext':'http://ext.com/?search=${query}'}"
        }
      }
    },
    "keybindings": [
      {
        "command": "extension.gotoDocument",
        "key": "ctrl+shift+h",
        "mac": "cmd+shift+h",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.gotoDocument",
          "group": "navigation@1"
        }
      ]
    },
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/python.json"
      }
    ],
    "languages": [
      {
        "id": "jupiter",
        "aliases": [
          "Jupiter",
          "jpl"
        ],
        "extensions": [
          ".jpl"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "python",
        "aliases": [
          "Python",
          "python"
        ],
        "extensions": [
          ".py"
        ]
      }
    ],
    "grammars": [
      {
        "language": "jupiter",
        "scopeName": "source.jupiter",
        "path": "./syntaxes/jupiter.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "js-yaml": "^3.12.2",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "mac-open": "^0.1.3",
    "open": "^7.3.0",
    "papaparse": "^5.3.0"
  }
}
